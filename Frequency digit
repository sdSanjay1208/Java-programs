import java.util.*;
class main
{
  public static void main(String[] args)
  {
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int digit[]=new int[10];
    while(n!=0)
    {
      int d=n%10;
      digit[d]++;
      n/=10;
    }
    for(int i=0;i<10;i++)
  {
    System.out.printf("%d occurs %d times\n",i,digit[i]);
  }
  }
}
    
